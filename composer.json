{
  "name": "konecnyjakub/clock",
  "description": "Collection of PSR-20 clocks",
  "license": "BSD-3-Clause",
  "keywords": [
    "clock", "psr-20"
  ],
  "authors": [
    {
      "name": "Jakub Konečný",
      "email": "jakub.konecny2@centrum.cz",
      "role": "Developer"
    }
  ],
  "support": {
    "sources": "https://gitlab.com/konecnyjakub/clock",
    "issues": "https://gitlab.com/konecnyjakub/clock/-/issues"
  },
  "require": {
    "php": ">=8.3.0",
    "psr/clock": "^1.0"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.4",
    "php-parallel-lint/php-console-highlighter": "^0.5",
    "squizlabs/php_codesniffer": "^3.10",
    "phpstan/phpstan": "^1.12",
    "konecnyjakub/mytester": "^6.0"
  },
  "provide": {
    "psr/clock-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Konecnyjakub\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Konecnyjakub\\Clock\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}